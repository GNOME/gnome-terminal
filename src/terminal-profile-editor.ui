                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_4">
                            <property name="title">Choose Palette Color 4</property>
                            <property name="show-editor">1</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_5">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 5</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_6">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 6</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_7">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 7</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwPreferencesRow">
                    <child>
                      <object class="GtkBox">
                        <property name="halign">center</property>
                        <property name="homogeneous">true</property>
                        <property name="margin-bottom">6</property>
                        <property name="margin-top">6</property>
                        <property name="spacing">6</property>
                        <property name="valign">center</property>
                        <child>
                          <object class="GtkColorButton" id="palette_8">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 8</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_9">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 9</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_10">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 10</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_11">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 11</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_12">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 12</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_13">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 13</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_14">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 14</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColorButton" id="palette_15">
                            <property name="show-editor">1</property>
                            <property name="title">Choose Palette Color 15</property>
                            <signal name="notify::rgba" handler="terminal_profile_editor_palette_index_changed" swapped="1"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwSwitchRow" id="show_bold_in_bright">
                    <property name="title" translatable="yes">Show Bold Text in Bright Colors</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title" translatable="yes">Cursor</property>
                <child>
                  <object class="AdwComboRow" id="cursor_shape">
                    <property name="title" translatable="yes">Shape</property>
                    <property name="expression">
                      <lookup name="title" type="TerminalPreferencesListItem"/>
                    </property>
                    <property name="model">
                      <object class="GListStore">
                        <property name="item-type">TerminalPreferencesListItem</property>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Block</property>
                            <property name="value">'block'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">I-Beam</property>
                            <property name="value">'ibeam'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Underline</property>
                            <property name="value">'underline'</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="AdwComboRow" id="cursor_blink">
                    <property name="title" translatable="yes">Blinking</property>
                    <property name="expression">
                      <lookup name="title" type="TerminalPreferencesListItem"/>
                    </property>
                    <property name="model">
                      <object class="GListStore">
                        <property name="item-type">TerminalPreferencesListItem</property>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Default</property>
                            <property name="value">'system'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Enabled</property>
                            <property name="value">'on'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Disabled</property>
                            <property name="value">'off'</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title" translatable="yes">Terminal Size</property>
                <child>
                  <object class="AdwSpinRow" id="columns">
                    <property name="title" translatable="yes">Columns</property>
                    <property name="numeric">1</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">16</property>
                        <property name="upper">511</property>
                        <property name="step-increment">1</property>
                        <property name="page-increment">10</property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="AdwSpinRow" id="rows">
                    <property name="title" translatable="yes">Rows</property>
                    <property name="numeric">1</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">4</property>
                        <property name="upper">511</property>
                        <property name="step-increment">1</property>
                        <property name="page-increment">10</property>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwSpinRow" id="cell_width">
                    <property name="title" translatable="yes">Cell Width Scale</property>
                    <property name="subtitle" translatable="yes">Increases the width of character cells</property>
                    <property name="numeric">1</property>
                    <property name="digits">2</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">1.0</property>
                        <property name="upper">2.0</property>
                        <property name="step-increment">.01</property>
                        <property name="page-increment">.1</property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>

<interface>
  <template class="TerminalProfileEditor" parent="AdwNavigationPage">
    <property name="title" translatable="yes">Edit Profile</property>
    <child>
      <object class="AdwToolbarView">
        <child type="top">
          <object class="AdwHeaderBar">
          </object>
        </child>
        <property name="content">
          <object class="AdwPreferencesPage" id="page">
            <property name="title" translatable="yes">Profile</property>
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwEntryRow" id="visible_name">
                    <property name="title" translatable="yes">Name</property>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow">
                    <property name="title" translatable="yes">Identifier</property>
                    <child type="suffix">
                      <object class="GtkLabel" id="uuid">
                        <property name="selectable">false</property>
                      </object>
                    </child>
		    <child type="suffix">
                      <object class="GtkButton">
                        <property name="valign">center</property>
			<property name="action-name">uuid.copy</property>
                        <property name="icon-name">edit-copy-symbolic</property>
                        <property name="tooltip-text" translatable="yes">Copy</property>
                        <style>
                          <class name="flat"/>
                        </style>
                      </object>
		    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title" translatable="yes">Font</property>
                <child>
                  <object class="AdwSwitchRow" id="use_system_font">
                    <property name="title" translatable="yes">Use System Font</property>
                  </object>
                </child>
                <child>
                  <object class="AdwActionRow" id="custom_font">
                    <property name="action-name">profile.select-custom-font</property>
                    <property name="activatable">yes</property>
                    <property name="title" translatable="yes">Custom Font</property>
                    <child type="suffix">
                      <object class="GtkBox">
                        <property name="hexpand">true</property>
                        <property name="spacing">12</property>
                        <child>
                          <object class="GtkLabel" id="custom_font_label">
                            <property name="hexpand">true</property>
                            <property name="xalign">1</property>
                            <property name="ellipsize">end</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkImage">
                            <property name="icon-name">go-next-symbolic</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwComboRow" id="allow_blinking_text">
                    <property name="title" translatable="yes">Allow Blinking Text</property>
                    <property name="expression">
                      <lookup name="title" type="TerminalPreferencesListItem"/>
                    </property>
                    <property name="model">
                      <object class="GListStore">
                        <property name="item-type">TerminalPreferencesListItem</property>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Always</property>
                            <property name="value">'always'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Never</property>
                            <property name="value">'never'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">When focused</property>
                            <property name="value">'focused'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">When unfocused</property>
                            <property name="value">'unfocused'</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title" translatable="yes">Colors</property>
                <child>
                  <object class="AdwSwitchRow" id="use_system_colors">
                    <property name="title" translatable="yes">Use Colors from Palette</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="sensitive" bind-source="use_system_colors" bind-property="active" bind-flags="invert-boolean"/>
                <child>
                  <object class="AdwComboRow" id="color_schemes">
                    <property name="sensitive" bind-source="use_system_colors" bind-property="active" bind-flags="invert-boolean"/>
                    <property name="title" translatable="yes">Use Colors from Scheme</property>
                  </object>
                </child>
                <child>
                  <object class="AdwExpanderRow">
                    <property name="title" translatable="yes">Override Default Color</property>
                    <child>
                      <object class="TerminalColorRow" id="default_color_text">
                        <property name="title" translatable="yes">Text</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Text Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="default_color_background">
                        <property name="title" translatable="yes">Background</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Background Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwExpanderRow">
                    <property name="title" translatable="yes">Override Bold Color</property>
                    <child type="suffix">
                      <object class="GtkSwitch" id="bold_color_set">
                        <property name="valign">center</property>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="bold_color_text">
                        <property name="title" translatable="yes">Text</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Bold Text Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwExpanderRow">
                    <property name="title" translatable="yes">Override Cursor Color</property>
                    <child type="suffix">
                      <object class="GtkSwitch" id="cursor_colors_set">
                        <property name="valign">center</property>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="cursor_color_text">
                        <property name="title" translatable="yes">Text</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Cursor Foreground Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="cursor_color_background">
                        <property name="title" translatable="yes">Background</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Cursor Background Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="AdwExpanderRow">
                    <property name="title" translatable="yes">Override Highlight Color</property>
                    <child type="suffix">
                      <object class="GtkSwitch" id="highlight_colors_set">
                        <property name="valign">center</property>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="highlight_color_text">
                        <property name="title" translatable="yes">Text</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Highlight Foreground Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                    <child>
                      <object class="TerminalColorRow" id="highlight_color_background">
                        <property name="title" translatable="yes">Background</property>
			<child internal-child="button">
			  <object class="GtkColorButton">
                            <property name="title" translatable="yes">Choose Highlight Background Color</property>
			  </object>
			</child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <property name="title" translatable="yes">Palette</property>

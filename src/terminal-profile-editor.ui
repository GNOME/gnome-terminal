                    <property name="model">
                      <object class="GListStore">
                        <property name="item-type">TerminalPreferencesListItem</property>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Regular</property>
                            <property name="value">'always'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">Overlay</property>
                            <property name="value">'overlay'</property>
                          </object>
                        </child>
                        <child>
                          <object class="TerminalPreferencesListItem">
                            <property name="title" translatable="yes">None</property>
                            <property name="value">'never'</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="AdwSwitchRow" id="kinetic_scrolling">
                    <property name="title" translatable="yes">Kinetic scrolling</property>
                    <property name="subtitle" translatable="yes">When used on touchscreens</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">
                <child>
                  <object class="AdwSwitchRow" id="scroll_on_output">
                    <property name="title" translatable="yes">Scroll on Output</property>
                  </object>
                </child>
                <child>
                  <object class="AdwSwitchRow" id="scroll_on_keystroke">
                    <property name="title" translatable="yes">Scroll on Keystroke</property>
                  </object>
                </child>
              </object>
            </child>
            <child>

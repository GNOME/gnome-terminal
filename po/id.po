msgstr "GEOMETRI"

#: ../src/gterminal.vala:225 ../src/terminal-options.c:1162
msgid "Set the window role"
msgstr "Tata peran jendela"

#: ../src/gterminal.vala:225 ../src/terminal-options.c:1163
